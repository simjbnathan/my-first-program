using System.Numerics;

int sum = CalculateSumOfEvenNumbers(1, 100);
Console.WriteLine(sum); 
static int CalculateSumOfEvenNumbers(int v1, int v2)
{
    int sum  = 0;
    for (int i = v1; i <= v2; i++)
    {
        if (i % 2 == 0)
        {
            sum += i;
        }
      
    }
    return sum;
}

